{
  "name": "storybook-playground",
  "version": "v0.1.0",
  "description": "A react component playground with storybook for quick component tinkering.",
  "repository": {
    "type": "git",
    "url": "git@github.com:luetkemj/storybook-playground.git"
  },
  "author": "luetkemj",
  "license": {
    "type": "MIT",
    "url": "https://github.com/luetkemj/storybook-playground/raw/master/LICENSE"
  },
  "main": "build/index.js",
  "scripts": {
    "build": "npm run clean:build && npm run clean:dist && npm run lint && npm run test && npm run webpack && babel src -d build",
    "clean:build": "rimraf build",
    "clean:coverage": "rimraf coverage",
    "clean:dist": "rimraf dist",
    "clean:node_modules": "rimraf node_modules",
    "clean": "npm run clean:build && npm run clean:coverage && npm run clean:dist && npm run clean:node_modules",
    "reinstall": "npm run clean && yarn",
    "lint:eslint": "eslint .",
    "lint": "npm run lint:eslint",
    "prepublish": "npm run build",
    "test:watch": "jest --watch --coverage",
    "test": "npm run lint && jest --coverage",
    "webpack": "./node_modules/.bin/webpack --config webpack.config.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "jest": {
    "verbose": true
  },
  "dependencies": {
    "react": "15.5.4"
  },
  "devDependencies": {
    "@kadira/storybook": "2.35.3",
    "babel-cli": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-plugin-transform-strict-mode": "6.24.1",
    "babel-preset-env": "1.4.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "css-loader": "0.28.1",
    "cz-conventional-changelog": "2.0.0",
    "cz-emoji": "0.1.0",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.0.1",
    "eslint-config-airbnb-base": "11.2.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.2",
    "eslint-plugin-react": "7.0.1",
    "jest": "20.0.1",
    "node-sass": "4.5.3",
    "react-dom": "15.5.4",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.5",
    "style-loader": "0.17.0",
    "webpack": "2.5.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-emoji"
    },
    "cz-emoji": {
      "types": [
        {
          "name": "build \tğŸ¤–  Changes that affect the build system or external dependencies",
          "value": "U+1F916"
        },
        {
          "name": "chore \tğŸ”¨  A chore change",
          "value": ":hammer:"
        },
        {
          "name": "ci \t\tğŸ‘·  Changes to our CI configuration files and scripts",
          "value": ":construction_worker:"
        },
        {
          "name": "docs \t\tğŸ“–  Documentation change",
          "value": ":book:"
        },
        {
          "name": "feature \tğŸŒŸ  A new feature",
          "value": ":star2:"
        },
        {
          "name": "fix \t\tğŸ  A bug fix",
          "value": ":beetle:"
        },
        {
          "name": "init \t\tâœ¨  Initalize project",
          "value": ":sparkles:"
        },
        {
          "name": "perf \t\tâ±ï¸  A code change that improves performance",
          "value": "U+23F1"
        },
        {
          "name": "refactor \tğŸ¨  A code refactoring change",
          "value": ":art:"
        },
        {
          "name": "revert \tğŸ˜±  Reverts a previous commit",
          "value": ":scream:"
        },
        {
          "name": "style \tğŸ’…  Changes that do not affect the meaning of the code (white-space, formatting, missing semi-colons, etc)",
          "value": ":nail_care:"
        },
        {
          "name": "test \t\tğŸ”¬  Adding missing tests or correcting existing tests",
          "value": ":microscope:"
        }
      ]
    }
  }
}
